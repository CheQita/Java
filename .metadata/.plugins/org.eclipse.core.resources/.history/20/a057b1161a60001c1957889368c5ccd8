package main;

import java.awt.Graphics2D;

public class Column {
	Cell[] cells;
	String name;
	int score;
	int nCols = 17;
	
	public Column(String _name) {
		name = _name;
		Cell nameCell = new Cell();
		nameCell.text = name;
		cells = new Cell[1 + nCols];
		cells[0] = nameCell;
		
		for(int i=1; i< 1 + nCols; i++) {
			cells[i] = new Cell();
		}
		
		
	}
	
	public void render(Graphics2D g) {
		
	}
}
