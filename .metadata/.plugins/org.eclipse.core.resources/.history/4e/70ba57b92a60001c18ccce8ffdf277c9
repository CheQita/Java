package main;

public class OptionsColumn extends Column{
	CellOption[] options;
	
	public OptionsColumn(String _name, int _posX, int _posY) {
		super(_name, _posX, _posY);
		options = new CellOption[] {
				CellOption.ETTOR,
				CellOption.TVÅOR,
				CellOption.TREOR,
				CellOption.FYROR,
				CellOption.FEMMOR,
				CellOption.SEXOR,
				CellOption.PAR,
				CellOption.TVÅ_PAR,
				CellOption.TRISS,
				CellOption.FYRTAL,
				CellOption.KÅK,
				CellOption.LITEN_STEGE,
				CellOption.STOR_STEGE,
				CellOption.CHANS,
				CellOption.YATSY,
				CellOption.SUMMA,
				
		};
	}


	public void addCells() {
		Cell cell1 = new Cell(posX, posY, ScoreBoard.colWidth, ScoreBoard.cellLength);
		cell1.setText(name);
		cell1.setEnabled(false);
		cells = new Cell[1 + nCols];
		cells[0] = cell1;
		for(int i=1; i< 1 + nCols; i++) {
			cells[i] = new Cell(posX, posY+ (ScoreBoard.cellLength*i), ScoreBoard.colWidth, ScoreBoard.cellLength);
		}
	}
	
	

}
